<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LostAndFoundPatras.Content.FoundVehiclesPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="This is the found vehicles page!"
                   VerticalOptions="CenterAndExpand" 
                   HorizontalOptions="CenterAndExpand"
                   FontSize="22"/>
            <Label x:Name="ToBeRemoved" VerticalOptions="StartAndExpand" HorizontalOptions="Center"/>
            <Label x:Name="TemporaryLocation" VerticalOptions="CenterAndExpand" HorizontalOptions="Center"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>